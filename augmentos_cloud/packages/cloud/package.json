{
  "name": "@augmentos/cloud",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && bun run sentry:sourcemaps && npm run sentry:sourcemaps",
    "postbuild": "cp -r src/public dist/public",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "staging": "NODE_ENV=staging tsx watch src/index.ts",
    "deploy": "bun run build && bun run start",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org pierce-50 --project augmentos-cloud-dev ./dist && sentry-cli sourcemaps upload --org pierce-50 --project augmentos-cloud-dev ./dist"
  },
  "author": "Isaiah Ballah",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.6.1",
    "@types/stream-buffers": "^3.0.4",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@augmentos/config": "workspace:*",
    "@augmentos/types": "workspace:*",
    "@sentry/bun": "^9.1.0",
    "@sentry/cli": "^2.42.1",
    "axios": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.40.0",
    "moment": "^2.29.4",
    "mongodb": "^6.13.0",
    "mongoose": "^6.5.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.0",
    "posthog-node": "^4.6.0",
    "uuid": "^11.0.5"
  }
}